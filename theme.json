{
  "$schema": "vscode://schemas/color-theme",
  "colors": {
    "activityBar.border": "#000",
    "dropdown.border": "#000",
    "editorWidget.border": "#000",
    "input.border": "#000",
    "panel.border": "#000",
    "sideBar.border": "#000",
    "sideBarSectionHeader.border": "#000",
    "statusBar.border": "#000",
    "terminal.border": "#000",
    "titleBar.border": "#000",

    "activityBar.dropBorder": "#060e1d",

    "activityBar.background": "#0c1423",
    "activityBarBadge.foreground": "#0c1423",
    "button.background": "#0c1423",
    "editorGroupHeader.tabsBackground": "#0c1423",
    "input.background": "#0c1423",
    "list.activeSelectionBackground": "#0c1423",
    "list.dropBackground": "#0c1423",
    "list.focusBackground": "#0c1423",
    "list.hoverBackground": "#0c1423",
    "list.inactiveSelectionBackground": "#0c1423",
    "panel.background": "#0c1423",
    "sideBarSectionHeader.background": "#0c1423",
    "statusBar.background": "#0c1423",
    "tab.border": "#0c1423",
    "tab.inactiveBackground": "#0c1423",
    "titleBar.activeBackground": "#0c1423",
    "titleBar.inactiveBackground": "#0c1423",

    "breadcrumbPicker.background": "#141c2b",
    "dropdown.listBackground": "#141c2b",
    "editorGroup.dropBackground": "#141c2b",
    "editorWidget.background": "#141c2b",
    "sideBar.background": "#141c2b",
    "sideBar.dropBackground": "#141c2b",
    "terminal.background": "#141c2b",

    "breadcrumb.background": "#17202e",
    "editor.background": "#17202e",
    "statusBarItem.hoverBackground": "#17202e",
    "tab.activeBackground": "#17202e",
    "tab.hoverBackground": "#17202e",

    "dropdown.background": "#1e293b",

    "tree.indentGuidesStroke": "#202a46",

    "editor.lineHighlightBorder": "#243248",
    "editorIndentGuide.background": "#243248",
    "editorRuler.foreground": "#243248",

    "editorIndentGuide.activeBackground": "#314462",
    "editorLineNumber.foreground": "#314462",

    "activityBar.inactiveForeground": "#384e6f",
    "panelTitle.inactiveForeground": "#384e6f",

    "activityBar.activeBorder": "#5c7cad",
    "activityBar.foreground": "#5c7cad",
    "activityBarBadge.background": "#5c7cad",
    "breadcrumb.foreground": "#5c7cad",
    "button.foreground": "#5c7cad",
    "editorWidget.foreground": "#5c7cad",
    "focusBorder": "#5c7cad",
    "foreground": "#5c7cad",
    "icon.foreground": "#5c7cad",
    "input.placeholderForeground": "#5c7cad",
    "panelTitle.activeForeground": "#5c7cad",
    "sideBar.foreground": "#5c7cad",
    "statusBar.foreground": "#5c7cad",
    "tab.activeBorderTop": "#5c7cad",
    "tab.inactiveForeground": "#5c7cad",
    "terminal.foreground": "#5c7cad",
    "titleBar.activeForeground": "#5c7cad",
    "titleBar.inactiveForeground": "#5c7cad",

    "breadcrumb.activeSelectionForeground": "#ced5df",
    "breadcrumb.focusForeground": "#ced5df",
    "dropdown.foreground": "#ced5df",
    "editorCursor.foreground": "#ced5df",
    "input.foreground": "#ced5df",
    "list.activeSelectionForeground": "#ced5df",
    "list.focusForeground": "#ced5df",
    "list.hoverForeground": "#ced5df",
    "list.inactiveSelectionForeground": "#ced5df",
    "sideBarSectionHeader.foreground": "#ced5df",
    "tab.activeForeground": "#ced5df",
    "tab.hoverForeground": "#ced5df",

    "gitDecoration.addedResourceForeground": "#A7F3D0",
    "gitDecoration.deletedResourceForeground": "#FECACA",
    "gitDecoration.ignoredResourceForeground": "#314562",
    "gitDecoration.modifiedResourceForeground": "#FDE68A"
  },
  "tokenColors": [
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "source",
      "settings": {
        "foreground": "#ced5df"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#f43f5e"
      }
    },
    {
      "name": "tag brackets",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#45608a"
      }
    },
    {
      "name": "tag properties",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#a7f3d0"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#5c7cad"
      }
    },
    {
      "scope": ["emphasis", "markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.bold", "markup.heading"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#c4b5fd"
      }
    },
    {
      "name": "strings",
      "scope": [
        "markup.deleted",
        "markup.inline.raw",
        "meta.embedded.assembly",
        "meta.preprocessor.string",
        "string",
        "string.tag",
        "string.value"
      ],
      "settings": {
        "foreground": "#fde68a"
      }
    },
    {
      "name": "functions",
      "scope": [
        "entity.name.operator.custom-literal",
        "source.powershell variable.other.member",
        "support.constant.handlebars",
        "support.function"
      ],
      "settings": {
        "foreground": "#6ee7b7"
      }
    },
    {
      "name": "css",
      "scope": [
        "constant.character.escape",
        "entity.name.tag.css",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.class.mixin.css",
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.parent-selector.css",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css",
        "entity.other.attribute-name.scss",
        "keyword.operator.quantifier.regexp",
        "source.css.less entity.other.attribute-name.id"
      ],
      "settings": {
        "foreground": "#93c5fd"
      }
    },
    {
      "name": "constants, variables",
      "scope": [
        "entity.name.type.enum",
        "entity.name.variable",
        "meta.definition.variable.name",
        "meta.object-literal.key",
        "meta.structure.dictionary.key.python",
        "source.coffee.embedded",
        "support.function.git-rebase",
        "support.type.property-name",
        "support.type.vendored.property-name",
        "support.variable",
        "variable",
        "variable.css",
        "variable.other.constant",
        "variable.other.enummember",
        "variable.other.less",
        "variable.scss"
      ],
      "settings": {
        "foreground": "#eaebff"
      }
    },
    {
      "name": "numbers, language, regex",
      "scope": [
        "constant.character.character-class.regexp",
        "constant.character.set.regexp",
        "constant.language.boolean",
        "constant.language.import-export-all.ts",
        "constant.language.null",
        "constant.numeric",
        "constant.other.character-class.regexp",
        "constant.other.character-class.set.regexp",
        "constant.sha.git-rebase",
        "entity.name.tag.js",
        "entity.name.tag.tsx",
        "keyword.operator.minus.exponent",
        "keyword.operator.plus.exponent",
        "keyword.other.unit",
        "markup.inserted",
        "meta.preprocessor.numeric",
        "string.regexp",
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#c4b5fd"
      }
    },
    {
      "name": "css, regex",
      "scope": [
        "constant.other.color.rgb-value",
        "constant.other.rgb-value",
        "keyword.control.anchor.regexp",
        "keyword.operator.negation.regexp",
        "keyword.operator.or.regexp",
        "punctuation.character.set.begin.regexp",
        "punctuation.character.set.end.regexp",
        "punctuation.definition.character-class.regexp",
        "punctuation.definition.group.assertion.regexp",
        "punctuation.definition.group.regexp",
        "support.constant.color",
        "support.constant.font-name",
        "support.constant.media",
        "support.constant.media-type",
        "support.constant.property-value",
        "support.other.parenthesis.regexp"
      ],
      "settings": {
        "foreground": "#a5b4fc"
      }
    },
    {
      "name": "keywords, operators",
      "scope": [
        "constant.character",
        "constant.language",
        "entity.name.function.preprocessor",
        "entity.name.operator",
        "entity.name.tag",
        "keyword",
        "keyword.control",
        "keyword.control",
        "keyword.operator",
        "markup.changed",
        "meta.diff.header",
        "meta.preprocessor",
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.section.embedded",
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.end.php",
        "punctuation.separator.key-value",
        "source.cpp keyword.operator.new",
        "storage",
        "storage.modifier",
        "storage.modifier.import.java",
        "storage.modifier.package.java",
        "storage.type",
        "variable.language",
        "variable.language.wildcard.java"
      ],
      "settings": {
        "foreground": "#f9a8d4"
      }
    },
    {
      "name": "types, classes",
      "scope": [
        "entity.name.class",
        "entity.name.namespace",
        "entity.name.scope-resolution",
        "entity.name.type",
        "entity.other.attribute",
        "entity.other.inherited-class",
        "meta.return-type",
        "meta.type.cast.expr",
        "meta.type.new.expr",
        "storage.type.annotation.groovy",
        "storage.type.annotation.java",
        "storage.type.boolean.go",
        "storage.type.byte.go",
        "storage.type.cs",
        "storage.type.error.go",
        "storage.type.generic.cs",
        "storage.type.generic.groovy",
        "storage.type.generic.java",
        "storage.type.groovy",
        "storage.type.java",
        "storage.type.modifier.cs",
        "storage.type.numeric.go",
        "storage.type.object.array.groovy",
        "storage.type.object.array.java",
        "storage.type.parameters.groovy",
        "storage.type.primitive.array.groovy",
        "storage.type.primitive.array.java",
        "storage.type.primitive.groovy",
        "storage.type.primitive.java",
        "storage.type.rune.go",
        "storage.type.string.go",
        "storage.type.token.java",
        "storage.type.uintptr.go",
        "storage.type.variable.cs",
        "support.class",
        "support.constant.dom",
        "support.constant.json",
        "support.constant.math",
        "support.type",
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#93c5fd"
      }
    }
  ],
  "semanticTokenColors": {
    "customLiteral": "#dcdcaa",
    "newOperator": "#c586c0",
    "numberLiteral": "#b5cea8",
    "stringLiteral": "#ce9178"
  },
  "semanticHighlighting": true
}
